}


<div class="container" style="background-color:white;padding:20px;">

    <div class="row">
        <div class="col-sm-1">
            <img src="~/img/Foto.jpeg" class="rounded float-start" width="50px" alt="Fotografia Perfil" />
        </div>
        <div class="col-sm-11">
            <h2 class="display-6">
                Jessica Guadalupe Delgado Chavez
            </h2>
            <p>
                <strong>TSU Desarrollo de Ssoftware Multiplataforma</strong>
            </p>
            @Html.ActionLink("DatosPersonales", "Index", "Home", null, new { @class = "btn btn-primary" })
            @Html.ActionLink("Experiencia Laboral", "ExperienciaLaboral", "ExperienciaLaboral", null, new { @class = "btn btn-primary" })
